{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dpp": "https://test.uncefact.org/vocabulary/untp/dpp/"
  },
  "@graph": [
    {
      "rdfs:label": "assuranceLevelCode",
      "@type": "rdfs:Class",
      "@id": "dpp:assuranceLevelCode",
      "rdfs:comment": "Code that describes the assurance level of the conformity assessment"
    },
    {
      "@id": "dpp:assuranceLevelCode#Self",
      "@type": "dpp:assuranceLevelCode",
      "dpp:Name": "Self",
      "rdfs:comment": " self-assessment"
    },
    {
      "@id": "dpp:assuranceLevelCode#Commercial",
      "@type": "dpp:assuranceLevelCode",
      "dpp:Name": "Commercial",
      "rdfs:comment": " conformity assessment by related body or under commercial contract"
    },
    {
      "@id": "dpp:assuranceLevelCode#Buyer",
      "@type": "dpp:assuranceLevelCode",
      "dpp:Name": "Buyer",
      "rdfs:comment": " conformity assessment by potential purchaser"
    },
    {
      "@id": "dpp:assuranceLevelCode#Membership",
      "@type": "dpp:assuranceLevelCode",
      "dpp:Name": "Membership",
      "rdfs:comment": " conformity assessment by industry representative body or membership body"
    },
    {
      "@id": "dpp:assuranceLevelCode#Unspecified",
      "@type": "dpp:assuranceLevelCode",
      "dpp:Name": "Unspecified",
      "rdfs:comment": " conformity assessment by party with unspecified relationship "
    },
    {
      "@id": "dpp:assuranceLevelCode#3rdParty",
      "@type": "dpp:assuranceLevelCode",
      "dpp:Name": "3rdParty",
      "rdfs:comment": " 3rd party (independent) conformity assessment"
    },
    {
      "rdfs:label": "conformityTopic",
      "@type": "rdfs:Class",
      "@id": "dpp:conformityTopic",
      "rdfs:comment": "A high level category scheme for conformity claims in the digital product passport"
    },
    {
      "@id": "dpp:conformityTopic#environment.energy",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "environment.energy",
      "rdfs:comment": "claims supporting clean energy transition"
    },
    {
      "@id": "dpp:conformityTopic#environment.emissions",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "environment.emissions",
      "rdfs:comment": "claims supporting GHG emissions reduction"
    },
    {
      "@id": "dpp:conformityTopic#environment.water",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "environment.water",
      "rdfs:comment": "claims supporting minimising water usage impact"
    },
    {
      "@id": "dpp:conformityTopic#environment.waste",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "environment.waste",
      "rdfs:comment": "claims supporting waste processing and reduction"
    },
    {
      "@id": "dpp:conformityTopic#environment.deforestation",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "environment.deforestation",
      "rdfs:comment": "claims supporting native forest restoration"
    },
    {
      "@id": "dpp:conformityTopic#environment.biodiversity",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "environment.biodiversity",
      "rdfs:comment": "claims supporting improved biodiversity outcomes"
    },
    {
      "@id": "dpp:conformityTopic#circularity.content",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "circularity.content",
      "rdfs:comment": "claims supporting the use of recycled content in products"
    },
    {
      "@id": "dpp:conformityTopic#circularity.design",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "circularity.design",
      "rdfs:comment": "claims supporting product design for circularity outcomes"
    },
    {
      "@id": "dpp:conformityTopic#social.labour",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "social.labour",
      "rdfs:comment": "claims supporting labour rights including fair wages"
    },
    {
      "@id": "dpp:conformityTopic#social.rights",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "social.rights",
      "rdfs:comment": "claims supporting human rights and anti-discrimination"
    },
    {
      "@id": "dpp:conformityTopic#social.community",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "social.community",
      "rdfs:comment": "claims supporting local community development"
    },
    {
      "@id": "dpp:conformityTopic#social.safety",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "social.safety",
      "rdfs:comment": "claims supporting process and product safety"
    },
    {
      "@id": "dpp:conformityTopic#governance.ethics",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "governance.ethics",
      "rdfs:comment": "claims supporting ethical conduct and corporate governance"
    },
    {
      "@id": "dpp:conformityTopic#governance.compliance",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "governance.compliance",
      "rdfs:comment": "claims supporting regulatory compliance including taxation and community protection"
    },
    {
      "@id": "dpp:conformityTopic#governance.transparency",
      "@type": "dpp:conformityTopic",
      "dpp:Name": "governance.transparency",
      "rdfs:comment": "claims supporting transparency and traceability"
    },
    {
      "rdfs:label": "eventTypeCode",
      "@type": "rdfs:Class",
      "@id": "dpp:eventTypeCode",
      "rdfs:comment": "A code describing the traceability event type - drawn from GS1 EPCIS 2.0"
    },
    {
      "@id": "dpp:eventTypeCode#aggregation",
      "@type": "dpp:eventTypeCode",
      "dpp:Name": "aggregation",
      "rdfs:comment": "event describing the grouping of products such as placing bales of cotton on a pallet"
    },
    {
      "@id": "dpp:eventTypeCode#transformation",
      "@type": "dpp:eventTypeCode",
      "dpp:Name": "transformation",
      "rdfs:comment": "event describing the consumption of input products to create output products in a manufacturing process such as spinning thread from cotton bales."
    },
    {
      "@id": "dpp:eventTypeCode#object",
      "@type": "dpp:eventTypeCode",
      "dpp:Name": "object",
      "rdfs:comment": "event describing an action on a single product such as an inspection or test"
    },
    {
      "@id": "dpp:eventTypeCode#transaction",
      "@type": "dpp:eventTypeCode",
      "dpp:Name": "transaction",
      "rdfs:comment": "event describing commercial transactions such as the sale of a collection of products from seller to buyer"
    },
    {
      "@id": "dpp:eventTypeCode#association",
      "@type": "dpp:eventTypeCode",
      "dpp:Name": "association",
      "rdfs:comment": "event describing the creation of relationships between products such as a bill of material of components in an assembly"
    },
    {
      "rdfs:label": "evidenceFormat",
      "@type": "rdfs:Class",
      "@id": "dpp:evidenceFormat",
      "rdfs:comment": "A code describing the format of the conformity evidence"
    },
    {
      "@id": "dpp:evidenceFormat#w3c_vc",
      "@type": "dpp:evidenceFormat",
      "dpp:Name": "w3c_vc",
      "rdfs:comment": "A W3C Verifiable Credential"
    },
    {
      "@id": "dpp:evidenceFormat#iso_mdl",
      "@type": "dpp:evidenceFormat",
      "dpp:Name": "iso_mdl",
      "rdfs:comment": "an ISO 108013 identity credential"
    },
    {
      "@id": "dpp:evidenceFormat#document",
      "@type": "dpp:evidenceFormat",
      "dpp:Name": "document",
      "rdfs:comment": "a binary document for human consumption such as a PDF"
    },
    {
      "@id": "dpp:evidenceFormat#website",
      "@type": "dpp:evidenceFormat",
      "dpp:Name": "website",
      "rdfs:comment": "a reference to an entry on a public website."
    },
    {
      "@id": "dpp:evidenceFormat#other",
      "@type": "dpp:evidenceFormat",
      "dpp:Name": "other",
      "rdfs:comment": "some other representation"
    },
    {
      "@id": "dpp:ProductPassport",
      "@type": "rdfs:Class",
      "rdfs:label": "ProductPassport",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/ProductPassport"
      },
      "rdfs:comment": "The ProductPassport is a comprehensive data structure that encapsulates various details pertaining to a product, including its identification details, who issued it, batch information, and different scores relating to sustainability and trustworthiness."
    },
    {
      "@id": "dpp:Party",
      "@type": "rdfs:Class",
      "rdfs:label": "Party",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Party"
      },
      "rdfs:comment": "The Party class represents an entity such as an organization, or a company that manufactured the product."
    },
    {
      "@id": "dpp:Facility",
      "@type": "rdfs:Class",
      "rdfs:label": "Facility",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Facility"
      },
      "rdfs:comment": "The physical site (eg farm or factory) where the product or materials was produced."
    },
    {
      "@id": "dpp:Product",
      "@type": "rdfs:Class",
      "rdfs:label": "Product",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Product"
      },
      "rdfs:comment": "The ProductInformation class encapsulates detailed information regarding a specific product, including its identification details, manufacturer, and other pertinent details."
    },
    {
      "@id": "dpp:Claim",
      "@type": "rdfs:Class",
      "rdfs:label": "Claim",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Claim"
      },
      "rdfs:comment": "The SustainabilityClaim class represents specific claims regarding the sustainability of a product, providing details about the metrics, thresholds, and evidences supporting the claim."
    },
    {
      "@id": "dpp:Metric",
      "@type": "rdfs:Class",
      "rdfs:label": "Metric",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Metric"
      },
      "rdfs:comment": "A specific measure of performance against the criteria that governs the claim.  Expressed as an array of metric (ie unit of emasure) / value (ie the actual numeric value) pairs.  "
    },
    {
      "@id": "dpp:TraceabilityEvent",
      "@type": "rdfs:Class",
      "rdfs:label": "TraceabilityEvent",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/TraceabilityEvent"
      },
      "rdfs:comment": "The TraceabilityEvent class represents a specific EPCIS event in the traceability chain of a product, including details about the event type and reference."
    },
    {
      "@id": "dpp:Material",
      "@type": "rdfs:Class",
      "rdfs:label": "Material",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Material"
      },
      "rdfs:comment": "The material class encapsulates details about the origin or source of raw materials in a product, including the country of origin and the mass fraction."
    },
    {
      "@id": "dpp:Evidence",
      "@type": "rdfs:Class",
      "rdfs:label": "Evidence",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Evidence"
      },
      "rdfs:comment": "Evidence to support a conformity or identity claim. "
    },
    {
      "@id": "dpp:Dimension",
      "@type": "rdfs:Class",
      "rdfs:label": "Dimension",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Dimension"
      },
      "rdfs:comment": "Overall (length, width, height) dimensions and weight/volume of an item."
    },
    {
      "@id": "dpp:Classification",
      "@type": "rdfs:Class",
      "rdfs:label": "Classification",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Classification"
      },
      "rdfs:comment": "A classification scheme and code / name representing a category value for a product, entity, or facility."
    },
    {
      "@id": "dpp:Identifier",
      "@type": "rdfs:Class",
      "rdfs:label": "Identifier",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Identifier"
      },
      "rdfs:comment": "An identifier of a party, product, or facility that is defined by an identifier scheme and idenfier value and, optinally, verification evidence "
    },
    {
      "@id": "dpp:Measure",
      "@type": "rdfs:Class",
      "rdfs:label": "Measure",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Measure"
      },
      "rdfs:comment": "The measure class defines a numeric measured value (eg 10) and a coded unit of measure (eg KG)."
    },
    {
      "@id": "dpp:Characteristic",
      "@type": "rdfs:Class",
      "rdfs:label": "Characteristic",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untp/dpp/Characteristic"
      },
      "rdfs:comment": "Product specific characteristics.  This class is an extension point for industry specific product characteristics or performance information such as clothing size or battery capacity."
    },
    {
      "@id": "dpp:id",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "id",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "dpp:ProductPassport"
      },
      "rdfs:comment": "A unique identifier (URI) assigned to the product passport."
    },
    {
      "@id": "dpp:issuedBy",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "issuedBy",
      "schema:rangeIncludes": {
        "@id": "dpp:Party"
      },
      "schema:domainIncludes": {
        "@id": "dpp:ProductPassport"
      },
      "rdfs:comment": "The Party that issued the product passport."
    },
    {
      "@id": "dpp:product",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "product",
      "schema:rangeIncludes": {
        "@id": "dpp:Product"
      },
      "schema:domainIncludes": {
        "@id": "dpp:ProductPassport"
      },
      "rdfs:comment": "Detailed information about the product encapsulated in a ProductInformation object."
    },
    {
      "@id": "dpp:guaranteeOfOriginCredential",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "guaranteeOfOriginCredential",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "dpp:ProductPassport"
      },
      "rdfs:comment": "A conformity credential issued by a trusted authority that confirms some or all of the claims made in this product passport"
    },
    {
      "@id": "dpp:trustScore",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "trustScore",
      "schema:rangeIncludes": {
        "@id": "xsd:decimal"
      },
      "schema:domainIncludes": {
        "@id": "dpp:ProductPassport"
      },
      "rdfs:comment": "An aggregate numeric metric that represents the level of trustworthiness associated with the product. This score is derived based on the credibility and reliability of the issuing bodies that substantiate the claims being made about the product. The calculation rules are defined in the UNTP trust graph specification."
    },
    {
      "@id": "dpp:sustainabilityScore",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "sustainabilityScore",
      "schema:rangeIncludes": {
        "@id": "xsd:decimal"
      },
      "schema:domainIncludes": {
        "@id": "dpp:ProductPassport"
      },
      "rdfs:comment": "An aggregate numeric metric calculated based on the various sustainability claims vs benchmarks associated with the product. It amalgamates scores assigned to individual sustainability claims, which are validated by various issuing bodies. The score provides a comprehensive view of the product's overall sustainability performance, giving users a quantifiable measure of the product's environmental and social impacts."
    },
    {
      "@id": "dpp:identifier",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "identifier",
      "schema:rangeIncludes": {
        "@id": "dpp:Identifier"
      },
      "schema:domainIncludes": [
        {
          "@id": "dpp:Party"
        },
        {
          "@id": "dpp:Facility"
        }
      ],
      "rdfs:comment": "A unique identifier (URI) assigned to the organization or company. (Link Resolver - GS1 company prefix?)"
    },
    {
      "@id": "dpp:name",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "name",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": [
        {
          "@id": "dpp:Party"
        },
        {
          "@id": "dpp:Facility"
        },
        {
          "@id": "dpp:Metric"
        }
      ],
      "rdfs:comment": "The name of the organization or company, represented as a text string."
    },
    {
      "@id": "dpp:location",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "location",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Facility"
      },
      "rdfs:comment": ""
    },
    {
      "@id": "dpp:operatedBy",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "operatedBy",
      "schema:rangeIncludes": {
        "@id": "dpp:Party"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Facility"
      },
      "rdfs:comment": "The Party entity responsible for operating the facility."
    },
    {
      "@id": "dpp:productIdentifier",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "productIdentifier",
      "schema:rangeIncludes": {
        "@id": "dpp:Identifier"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "An array of unique identifiers assigned to the product or model. "
    },
    {
      "@id": "dpp:batchIdentifier",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "batchIdentifier",
      "schema:rangeIncludes": {
        "@id": "dpp:Identifier"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "Information regarding the specific production batch of the product."
    },
    {
      "@id": "dpp:itemIdentifier",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "itemIdentifier",
      "schema:rangeIncludes": {
        "@id": "dpp:Identifier"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "An array of identifiers representing a specific serialised item of the product."
    },
    {
      "@id": "dpp:classification",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "classification",
      "schema:rangeIncludes": {
        "@id": "dpp:Classification"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "A code representing the product's class, typically using the UN CPC (United Nations Central Product Classification) https://unstats.un.org/unsd/classifications/Econ/cpc"
    },
    {
      "@id": "dpp:modelName",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "modelName",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "The model name or number of the product, represented as text."
    },
    {
      "@id": "dpp:image",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "image",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "A unique identifier (URI) pointing to an image of the product."
    },
    {
      "@id": "dpp:description",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "description",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "A textual description providing details about the product."
    },
    {
      "@id": "dpp:furtherInformation",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "furtherInformation",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "A URL pointing to further human readable information about the product."
    },
    {
      "@id": "dpp:manufacturedDate",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "manufacturedDate",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "The ISO 8601 date on which the product batch was manufactured."
    },
    {
      "@id": "dpp:dimension",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "dimension",
      "schema:rangeIncludes": {
        "@id": "dpp:Dimension"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "The physical dimensions of the product. Not every dimension is relevant to every products.  For example bulk materials may have wieght and volume but not length, with, or height."
    },
    {
      "@id": "dpp:characteristic",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "characteristic",
      "schema:rangeIncludes": {
        "@id": "dpp:Characteristic"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": ""
    },
    {
      "@id": "dpp:manufacturer",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "manufacturer",
      "schema:rangeIncludes": {
        "@id": "dpp:Party"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "The Party entity that manufactured the product."
    },
    {
      "@id": "dpp:manufacturedAt",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "manufacturedAt",
      "schema:rangeIncludes": {
        "@id": "dpp:Facility"
      },
      "schema:domainIncludes": {
        "@id": "dpp:Product"
      },
      "rdfs:comment": "The Facility where the product batch was manufactured."
    },
    {
      "@id": "dpp:materialsProvenance",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "materialsProvenance",
