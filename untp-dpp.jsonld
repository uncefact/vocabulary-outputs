{
  "@context": {
    "untp-dpp": "https://test.uncefact.org/vocabulary/untp/dpp/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "untp-core": "https://test.uncefact.org/vocabulary/untp/core/"
  },
  "@graph": [
    {
      "@id": "untp-dpp:DigitalProductPassport",
      "@type": "rdfs:Class",
      "rdfs:comment": "The ProductPassport is a comprehensive data structure that encapsulates various details pertaining to a product, including its identification details, who issued it, batch information, provernance information, circularity information and a set of verifable product confomrity & sistainability claims.  ",
      "rdfs:label": "DigitalProductPassport",
      "rdfs:subClassOf": "untp-core:VerifiableCredential"
    },
    {
      "@id": "untp-dpp:Product",
      "@type": "rdfs:Class",
      "rdfs:comment": "The ProductInformation class encapsulates detailed information regarding a specific product, including its identification details, manufacturer, and other pertinent details. ",
      "rdfs:label": "Product",
      "rdfs:subClassOf": "untp-core:Product"
    },
    {
      "@id": "untp-dpp:Declaration",
      "@type": "rdfs:Class",
      "rdfs:comment": "A declaration of conformance with one or more criteria from a specific standard or regulation.  ",
      "rdfs:label": "Declaration",
      "rdfs:subClassOf": "untp-core:Declaration"
    },
    {
      "@id": "untp-dpp:TraceabilityEvent",
      "@type": "rdfs:Class",
      "rdfs:comment": "The TraceabilityEvent class represents a specific EPCIS event in the traceability chain of a product, including details about the event type and reference.",
      "rdfs:label": "TraceabilityEvent"
    },
    {
      "@id": "untp-dpp:@context",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:DigitalProductPassport"
        }
      ],
      "rdfs:comment": [
        "A list of JSON-LD context URIs that define the seamntic meaning of prperties within the credential. "
      ],
      "rdfs:label": "@context"
    },
    {
      "@id": "untp-dpp:id",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:DigitalProductPassport"
        },
        {
          "@id": "untp-dpp:Product"
        },
        {
          "@id": "untp-dpp:Declaration"
        }
      ],
      "rdfs:comment": [
        "A unique identifier (URI) assigned to the product passport. May be a UUID",
        "The globally unique ID of the entity as a resolvable URL according to ISO 18975. ExamplesProduct - id.gs1.org/01/09520123456788/21/12345  Party - abr.business.gov.au/ABN/View?abn=90664869327. Facility - did:web:facilities.example-company.com:123. ",
        "A unique identifier for the delcaration. Often this will be an extension of the product.id or attestation.id. But could be a UUID."
      ],
      "rdfs:label": "id"
    },
    {
      "@id": "untp-dpp:issuer",
      "schema:rangeIncludes": {
        "@id": "untp-core:CredentialIssuer"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:DigitalProductPassport"
        }
      ],
      "rdfs:comment": [
        "The organisation that is the isuer of this VC. Note that the \"id\" property MUST be a W3C DID.  Other identifiers such as tax registration numbers can be listed in the \"otherIdentifiers\" property."
      ],
      "rdfs:label": "issuer"
    },
    {
      "@id": "untp-dpp:validFrom",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:DigitalProductPassport"
        }
      ],
      "rdfs:comment": [
        "The date and time from which the credential is valid."
      ],
      "rdfs:label": "validFrom"
    },
    {
      "@id": "untp-dpp:validUntil",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:DigitalProductPassport"
        }
      ],
      "rdfs:comment": [
        "The expiry date (if applicable) of this verifiable credential."
      ],
      "rdfs:label": "validUntil"
    },
    {
      "@id": "untp-dpp:credentialSubject",
      "schema:rangeIncludes": {
        "@id": "untp-dpp:Product"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:DigitalProductPassport"
        }
      ],
      "rdfs:comment": [
        "The subject of a digital product passport credential is the identified product. "
      ],
      "rdfs:label": "credentialSubject"
    },
    {
      "@id": "untp-dpp:name",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The registered name of the entity within the identifier scheme.  Examplesproduct - EV battery 50kWhparty - Sample Company Pty Ltd. facility - Greenacres battery factory"
      ],
      "rdfs:label": "name"
    },
    {
      "@id": "untp-dpp:idValue",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The value of the identifier within the scheme. Examplesproduct - 09520123.456788party - 90664869327. facility - 123. "
      ],
      "rdfs:label": "idValue"
    },
    {
      "@id": "untp-dpp:idScheme",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The identifier scheme ID.  Exampleproduct - id.gs1.org/01. party - business.gov.au/abn  facility - did:web:facilities.acme.com. "
      ],
      "rdfs:label": "idScheme"
    },
    {
      "@id": "untp-dpp:idSchemeName",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The name of the identifier scheme - examples product - GS1 SGTIN. party - ABN. facility - ACME  "
      ],
      "rdfs:label": "idSchemeName"
    },
    {
      "@id": "untp-dpp:serialNumber",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "A number or code representing a specific serialised item of the product. Unique within product class."
      ],
      "rdfs:label": "serialNumber"
    },
    {
      "@id": "untp-dpp:batchNumber",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "Identifier of the specific production batch of the product.  Unique within the product class."
      ],
      "rdfs:label": "batchNumber"
    },
    {
      "@id": "untp-dpp:productImage",
      "schema:rangeIncludes": {
        "@id": "untp-core:Link"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "Reference information (location, type, name) of an image of the product."
      ],
      "rdfs:label": "productImage"
    },
    {
      "@id": "untp-dpp:description",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "A textual description providing details about the product."
      ],
      "rdfs:label": "description"
    },
    {
      "@id": "untp-dpp:productCategory",
      "schema:rangeIncludes": {
        "@id": "untp-core:Classification"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "A code representing the product's class, typically using the UN CPC (United Nations Central Product Classification) https://unstats.un.org/unsd/classifications/Econ/cpc"
      ],
      "rdfs:label": "productCategory"
    },
    {
      "@id": "untp-dpp:furtherInformation",
      "schema:rangeIncludes": {
        "@id": "untp-core:Link"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "A URL pointing to further human readable information about the product."
      ],
      "rdfs:label": "furtherInformation"
    },
    {
      "@id": "untp-dpp:producedByParty",
      "schema:rangeIncludes": {
        "@id": "untp-core:Entity"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The Party entity that manufactured the product."
      ],
      "rdfs:label": "producedByParty"
    },
    {
      "@id": "untp-dpp:producedAtFacility",
      "schema:rangeIncludes": {
        "@id": "untp-core:Entity"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The Facility where the product batch was produced / manufactured."
      ],
      "rdfs:label": "producedAtFacility"
    },
    {
      "@id": "untp-dpp:dimensions",
      "schema:rangeIncludes": {
        "@id": "untp-core:Dimension"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The physical dimensions of the product. Not every dimension is relevant to every products.  For example bulk materials may have wieght and volume but not length, with, or height.\"weight\":{\"value\":10, \"unit\":\"KGM\"}"
      ],
      "rdfs:label": "dimensions"
    },
    {
      "@id": "untp-dpp:productionDate",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The ISO 8601 date on which the product batch was manufactured."
      ],
      "rdfs:label": "productionDate"
    },
    {
      "@id": "untp-dpp:countryOfProduction",
      "schema:rangeIncludes": {
        "@id": "untp-dpp:countryCode"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "The country in which this item was produced / manufactured."
      ],
      "rdfs:label": "countryOfProduction"
    },
    {
      "@id": "untp-dpp:characteristics",
      "schema:rangeIncludes": {
        "@id": "untp-core:Characteristic"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "Product specific characteristics.  This class is an extension point for industry specific product characteristics or performance information such as clothing size or battery capacity."
      ],
      "rdfs:label": "characteristics"
    },
    {
      "@id": "untp-dpp:materialsProvenance",
      "schema:rangeIncludes": {
        "@id": "untp-core:Material"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "An array of Provenance objects providing details on the origin and mass fraction of components or ingredients of the product batch. "
      ],
      "rdfs:label": "materialsProvenance"
    },
    {
      "@id": "untp-dpp:conformityDeclarations",
      "schema:rangeIncludes": {
        "@id": "untp-dpp:Declaration"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "An array of claim objects representing various product conformity claims about the product / batch.  These can be sustainability claims, circularity claims, or any other claim type within the conformity topic list."
      ],
      "rdfs:label": "conformityDeclarations"
    },
    {
      "@id": "untp-dpp:circularityScorecard",
      "schema:rangeIncludes": {
        "@id": "untp-core:CircularityPerformance"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "A circularity performance scorecard"
      ],
      "rdfs:label": "circularityScorecard"
    },
    {
      "@id": "untp-dpp:emissionsScorecard",
      "schema:rangeIncludes": {
        "@id": "untp-core:EmissionsPerformance"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "An emissions performacne scorecard"
      ],
      "rdfs:label": "emissionsScorecard"
    },
    {
      "@id": "untp-dpp:traceabilityInformation",
      "schema:rangeIncludes": {
        "@id": "untp-dpp:TraceabilityEvent"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Product"
        }
      ],
      "rdfs:comment": [
        "An array of TraceabilityEvent objects detailing EPCIS events related to the traceability of the product batch. SHOULD be provided for any product passport issued at batch or item level. "
      ],
      "rdfs:label": "traceabilityInformation"
    },
    {
      "@id": "untp-dpp:referenceStandard",
      "schema:rangeIncludes": {
        "@id": "untp-core:Standard"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Declaration"
        }
      ],
      "rdfs:comment": [
        "The reference to the standard that defines the specification / criteria"
      ],
      "rdfs:label": "referenceStandard"
    },
    {
      "@id": "untp-dpp:referenceRegulation",
      "schema:rangeIncludes": {
        "@id": "untp-core:Regulation"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Declaration"
        }
      ],
      "rdfs:comment": [
        "The reference to the regulation that defines the assessment criteria"
      ],
      "rdfs:label": "referenceRegulation"
    },
    {
      "@id": "untp-dpp:assessmentCriteria",
      "schema:rangeIncludes": {
        "@id": "untp-core:Criterion"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Declaration"
        }
      ],
      "rdfs:comment": [
        "The specification against which the assessment is made."
      ],
      "rdfs:label": "assessmentCriteria"
    },
    {
      "@id": "untp-dpp:declaredValues",
      "schema:rangeIncludes": {
        "@id": "untp-core:Metric"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Declaration"
        }
      ],
      "rdfs:comment": [
        "The list of specific values measured as part of this assessment (eg tensile strength)"
      ],
      "rdfs:label": "declaredValues"
    },
    {
      "@id": "untp-dpp:compliance",
      "schema:rangeIncludes": {
        "@id": "xsd:boolean"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Declaration"
        }
      ],
      "rdfs:comment": [
        "An indicator of whether or not the assessment meets the specification."
      ],
      "rdfs:label": "compliance"
    },
    {
      "@id": "untp-dpp:conformityTopic",
      "schema:rangeIncludes": {
        "@id": "untp-dpp:conformityTopicCode"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Declaration"
        }
      ],
      "rdfs:comment": [
        "The conformity topic category for this assessment (eg vocabulary.uncefact.org/sustainability/emissions)"
      ],
      "rdfs:label": "conformityTopic"
    },
    {
      "@id": "untp-dpp:conformityEvidence",
      "schema:rangeIncludes": {
        "@id": "untp-core:SecureLink"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:Declaration"
        }
      ],
      "rdfs:comment": [
        "A URI pointing to the evidence supporting the claim. SHOULD be a URL to a UNTP Digital COnformity Credential (DCC)"
      ],
      "rdfs:label": "conformityEvidence"
    },
    {
      "@id": "untp-dpp:eventReference",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:TraceabilityEvent"
        }
      ],
      "rdfs:comment": [
        "A URI pointing to the detailed information about the EPCIS event. SHOULD be a URL lik to a UNTP Digital Traceability Event (DTE) "
      ],
      "rdfs:label": "eventReference"
    },
    {
      "@id": "untp-dpp:eventType",
      "schema:rangeIncludes": {
        "@id": "untp-dpp:eventTypeCode"
      },
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "schema:domainIncludes": [
        {
          "@id": "untp-dpp:TraceabilityEvent"
        }
      ],
      "rdfs:comment": [
        "A code representing the type of EPCIS event. ObjectEvent, AggregationEvent, TransactionEvent, TransformationEvent, ObjectEvent."
      ],
      "rdfs:label": "eventType"
    },
    {
      "@id": "untp-dpp:eventTypeCode#aggregation",
      "@type": "untp-dpp:eventTypeCode",
      "untp-dpp:Name": "aggregation",
      "rdfs:comment": "event describing the grouping of products such as placing bales of cotton on a pallet"
    },
    {
      "@id": "untp-dpp:eventTypeCode#transformation",
      "@type": "untp-dpp:eventTypeCode",
      "untp-dpp:Name": "transformation",
      "rdfs:comment": "event describing the consumption of input products to create output products in a manufacturing process such as spinning thread from cotton bales."
    },
    {
      "@id": "untp-dpp:eventTypeCode#object",
      "@type": "untp-dpp:eventTypeCode",
      "untp-dpp:Name": "object",
      "rdfs:comment": "event describing an action on a single product such as an inspection or test"
    },
    {
      "@id": "untp-dpp:eventTypeCode#transaction",
      "@type": "untp-dpp:eventTypeCode",
      "untp-dpp:Name": "transaction",
      "rdfs:comment": "event describing commercial transactions such as the sale of a collection of products from seller to buyer"
    },
    {
      "@id": "untp-dpp:eventTypeCode#association",
      "@type": "untp-dpp:eventTypeCode",
      "untp-dpp:Name": "association",
      "rdfs:comment": "event describing the creation of relationships between products such as a bill of material of components in an assembly"
    }
  ]
}