{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "untp": "https://test.uncefact.org/vocabulary/untp"
  },
  "@graph": [
    {
      "@id": "untp:VerifiableCredential",
      "@type": "rdfs:Class",
      "rdfs:label": "VerifiableCredential",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpVerifiableCredential"
      },
      "rdfs:comment": "A verifiable credential is a digital and verifiable version of everyday credentials such as certificates and licenses.  It conforms to the W3C Verifiable Credentials Data Model v2.0 (VCDM)"
    },
    {
      "@id": "untp:Party",
      "@type": "rdfs:Class",
      "rdfs:label": "Party",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpParty"
      },
      "rdfs:comment": "The Party class represents an entity such as an organization, or a company that manufactured the product."
    },
    {
      "@id": "untp:Facility",
      "@type": "rdfs:Class",
      "rdfs:label": "Facility",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpFacility"
      },
      "rdfs:comment": "The physical site (eg farm or factory) where the product or materials was produced."
    },
    {
      "@id": "untp:Metric",
      "@type": "rdfs:Class",
      "rdfs:label": "Metric",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpMetric"
      },
      "rdfs:comment": "A specific measure of performance against the criteria that governs the claim.  Expressed as an array of metric (ie unit of emasure) / value (ie the actual numeric value) pairs.  "
    },
    {
      "@id": "untp:Dimension",
      "@type": "rdfs:Class",
      "rdfs:label": "Dimension",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpDimension"
      },
      "rdfs:comment": "Overall (length, width, height) dimensions and weight/volume of an item."
    },
    {
      "@id": "untp:Classification",
      "@type": "rdfs:Class",
      "rdfs:label": "Classification",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpClassification"
      },
      "rdfs:comment": "A classification scheme and code / name representing a category value for a product, entity, or facility."
    },
    {
      "@id": "untp:Identifier",
      "@type": "rdfs:Class",
      "rdfs:label": "Identifier",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpIdentifier"
      },
      "rdfs:comment": "An identifier of a party, product, or facility that is defined by an identifier scheme and idenfier value and, optinally, verification evidence "
    },
    {
      "@id": "untp:Measure",
      "@type": "rdfs:Class",
      "rdfs:label": "Measure",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpMeasure"
      },
      "rdfs:comment": "The measure class defines a numeric measured value (eg 10) and a coded unit of measure (eg KG)."
    },
    {
      "@id": "untp:BinaryFile",
      "@type": "rdfs:Class",
      "rdfs:label": "BinaryFile",
      "schema:source": {
        "@id": "https://test.uncefact.org/vocabulary/untpBinaryFile"
      },
      "rdfs:comment": "A file representing a data snapshot that is used to infomr the conformity assessment."
    },
    {
      "@id": "untp:@context",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "@context",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:VerifiableCredential"
      },
      "rdfs:comment": "A list of JSON-LD contect URIs that define the seamntic meaning of prperties within the credential. "
    },
    {
      "@id": "untp:id",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "id",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:VerifiableCredential"
      },
      "rdfs:comment": "A unique identifier (URI) assigned to the product passport."
    },
    {
      "@id": "untp:issuer",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "issuer",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:VerifiableCredential"
      },
      "rdfs:comment": "The W3C DID of the Party that issued the verifiable credential."
    },
    {
      "@id": "untp:type",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "type",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:VerifiableCredential"
      },
      "rdfs:comment": "The credential type.  Must include the string \"VerifiableCredential\" and additionally the specific credential type (eg \"UNTPDigitalProductPassport\")"
    },
    {
      "@id": "untp:validFrom",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "validFrom",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:VerifiableCredential"
      },
      "rdfs:comment": "The date and time from which the credential is valid."
    },
    {
      "@id": "untp:validUntil",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "validUntil",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:VerifiableCredential"
      },
      "rdfs:comment": "The expiry date (if applicable) of this verifiable credential."
    },
    {
      "@id": "untp:credentialStatus",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "credentialStatus",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:VerifiableCredential"
      },
      "rdfs:comment": "The status of the verifiable credential - must be an object that conforms to the W3C VCDM 2.0 Status object - https://www.w3.org/TR/vc-data-model-2.0/#status "
    },
    {
      "@id": "untp:partyID",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "partyID",
      "schema:rangeIncludes": {
        "@id": "untp:Identifier"
      },
      "schema:domainIncludes": {
        "@id": "untp:Party"
      },
      "rdfs:comment": "The decentralised identifier of the party - must be a W3C DID."
    },
    {
      "@id": "untp:name",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "name",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": [
        {
          "@id": "untp:Party"
        },
        {
          "@id": "untp:Facility"
        },
        {
          "@id": "untp:Metric"
        }
      ],
      "rdfs:comment": "The name of the organization or company, represented as a text string."
    },
    {
      "@id": "untp:facilityID",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "facilityID",
      "schema:rangeIncludes": {
        "@id": "untp:Identifier"
      },
      "schema:domainIncludes": {
        "@id": "untp:Facility"
      },
      "rdfs:comment": "A unique identifier (URI) assigned to the facility. (Link Resolver - GS1 GLN?)"
    },
    {
      "@id": "untp:geoLocation",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "geoLocation",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:Facility"
      },
      "rdfs:comment": "The gelocation of the facility as a URI - typically a plus code (see https://maps.google.com/pluscodes/) "
    },
    {
      "@id": "untp:operatedBy",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "operatedBy",
      "schema:rangeIncludes": {
        "@id": "untp:Party"
      },
      "schema:domainIncludes": {
        "@id": "untp:Facility"
      },
      "rdfs:comment": "The Party entity responsible for operating the facility."
    },
    {
      "@id": "untp:value",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "value",
      "schema:rangeIncludes": {
        "@id": "untp:Measure"
      },
      "schema:domainIncludes": [
        {
          "@id": "untp:Metric"
        },
        {
          "@id": "untp:Measure"
        }
      ],
      "rdfs:comment": "A numeric value representing the measurement or evaluation outcome for the claim."
    },
    {
      "@id": "untp:accuracy",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "accuracy",
      "schema:rangeIncludes": {
        "@id": "xsd:decimal"
      },
      "schema:domainIncludes": {
        "@id": "untp:Metric"
      },
      "rdfs:comment": "A percentage represented as a numeric between 0 and 1 indicating the rage of accuracy of the claimed value (eg 0.05 means that the actual value is within 5% of the claimed value.)"
    },
    {
      "@id": "untp:weight",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "weight",
      "schema:rangeIncludes": {
        "@id": "untp:Measure"
      },
      "schema:domainIncludes": {
        "@id": "untp:Dimension"
      },
      "rdfs:comment": "the weight of the product"
    },
    {
      "@id": "untp:length",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "length",
      "schema:rangeIncludes": {
        "@id": "untp:Measure"
      },
      "schema:domainIncludes": {
        "@id": "untp:Dimension"
      },
      "rdfs:comment": "The length of the product or packaging"
    },
    {
      "@id": "untp:width",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "width",
      "schema:rangeIncludes": {
        "@id": "untp:Measure"
      },
      "schema:domainIncludes": {
        "@id": "untp:Dimension"
      },
      "rdfs:comment": "The width of the product or packaging"
    },
    {
      "@id": "untp:height",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "height",
      "schema:rangeIncludes": {
        "@id": "untp:Measure"
      },
      "schema:domainIncludes": {
        "@id": "untp:Dimension"
      },
      "rdfs:comment": "The height of the product or packaging"
    },
    {
      "@id": "untp:volume",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "volume",
      "schema:rangeIncludes": {
        "@id": "untp:Measure"
      },
      "schema:domainIncludes": {
        "@id": "untp:Dimension"
      },
      "rdfs:comment": "The displacement volume of the product."
    },
    {
      "@id": "untp:schemeID",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "schemeID",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:Classification"
      },
      "rdfs:comment": "Classification scheme  - eg https://unstats.un.org/unsd/classifications/Econ/cpc "
    },
    {
      "@id": "untp:classifierValue",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "classifierValue",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:Classification"
      },
      "rdfs:comment": "classifier value within the scheme - eg \"01211\" in UN CPC"
    },
    {
      "@id": "untp:classifierName",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "classifierName",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:Classification"
      },
      "rdfs:comment": "Name of the classifier - eg \"Asparagus\" for code \"01211\" in UNCPC"
    },
    {
      "@id": "untp:classifierID",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "classifierID",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:Classification"
      },
      "rdfs:comment": "Linked data URL to a web vocabulary entery for this classificaiton code. When this property is provided, the scheme, value, and name properties of the classifer are not required.  eg https://vocabulary.uncefact.org/unlocode#AUBNE represensign the port of Brisbane in the UN/LOCODE classification scheme."
    },
    {
      "@id": "untp:identifierScheme",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "identifierScheme",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:Identifier"
      },
      "rdfs:comment": "the identifier scheme as defined by the registrar that manages the identifier registry. If the identifier scheme is registered with UNTP then this URI can use used to dicsover the resolution method (to get more data) and the verification method (to prove ownership)."
    },
    {
      "@id": "untp:identifierValue",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "identifierValue",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:Identifier"
      },
      "rdfs:comment": "The value of the identifier within the scheme"
    },
    {
      "@id": "untp:unit",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "unit",
      "schema:rangeIncludes": {
        "@id": "untp:unitOfMeasure"
      },
      "schema:domainIncludes": {
        "@id": "untp:Measure"
      },
      "rdfs:comment": "Unit of measure drawn from the UNECE rec20 measure code list."
    },
    {
      "@id": "untp:fileHash",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "fileHash",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:BinaryFile"
      },
      "rdfs:comment": "The MD5 hash of the file."
    },
    {
      "@id": "untp:fileLocation",
      "@type": [
        "rdf:Property",
        "owl:DatatypeProperty"
      ],
      "rdfs:label": "fileLocation",
      "schema:rangeIncludes": {
        "@id": "xsd:string"
      },
      "schema:domainIncludes": {
        "@id": "untp:BinaryFile"
      },
      "rdfs:comment": "The location of the evidence file."
    },
    {
      "@id": "untp:fileType",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:label": "fileType",
      "schema:rangeIncludes": {
        "@id": "untp:mimeType"
      },
      "schema:domainIncludes": {
        "@id": "untp:BinaryFile"
      },
      "rdfs:comment": "The type of file, represented as a MIME type."
    }
  ],
  "jargon": {}
}